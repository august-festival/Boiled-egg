  "bugs": {
    "url": "https://github.com/august-festival/boiled-egg/issues"
  },
  "homepage": "https://github.com/august-festival/boiled-egg#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "config": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.16.3",
    "express-graphql": "^0.6.12",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
